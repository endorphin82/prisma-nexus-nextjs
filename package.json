{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port=3002",
    "build": "next build",
    "start": "next start",
    "now-build": "next build && yarn db:migrate",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "introspect:prisma": "prisma introspect",
    "generate:prisma": "prisma generate",
    "studio:prisma": "npx prisma studio --experimental",
    "migration_save:prisma": "prisma migrate save --experimental",
    "migration_UP:prisma": "prisma migrate up --experimental",
    "generate:nexus1": "ts-node --transpile-only gqlapi/schema_prs",
    "generate:nexus2": "ts-node --transpile-only -P nexus.tsconfig.json gqlapi/schema_prs",
    "generateHoocs": "graphql-codegen",
    "watch-generate": "graphql-codegen --watch",
    "clear": "find ./components -name \"__generated__\" -exec rm -rf '{}' +"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.0",
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@prisma/client": "^2.0.0-beta.5",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-server-micro": "^2.13.1",
    "classnames": "^2.2.6",
    "dataloader": "^2.0.0",
    "date-fns": "^2.13.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "gray-matter": "^4.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "knex": "^0.21.1",
    "micro-cors": "^0.1.1",
    "mongoose": "^5.9.15",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "next": "9.3.5",
    "nexus": "^0.21.0",
    "nexus-prisma": "^0.13.0",
    "postcss-preset-env": "^6.7.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "remark": "^12.0.0",
    "remark-html": "^11.0.2",
    "sass": "^1.26.5",
    "sequelize": "^5.21.9",
    "sequelize-cli": "^5.5.1",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.15.0",
    "@graphql-codegen/near-operation-file-preset": "^1.15.0",
    "@graphql-codegen/typescript": "^1.15.0",
    "@graphql-codegen/typescript-operations": "^1.15.0",
    "@graphql-codegen/typescript-react-apollo": "^1.15.0",
    "@prisma/cli": "^2.0.0-beta.5",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.2"
  }
}
